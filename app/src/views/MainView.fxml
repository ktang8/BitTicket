<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="360.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="logout" layoutX="551.0" layoutY="23.0" mnemonicParsing="false" text="Logout" />
      <TabPane layoutX="29.0" layoutY="56.0" prefHeight="286.0" prefWidth="588.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="activetickets" text="Active Tickets">
               <content>
                  <TableView prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="at_id" prefWidth="75.0" text="ID" />
                      <TableColumn fx:id="at_name" prefWidth="96.0" text="Name" />
                        <TableColumn fx:id="at_priority" prefWidth="116.0" text="Priority" />
                        <TableColumn fx:id="at_status" minWidth="2.0" prefWidth="84.0" text="Status" />
                        <TableColumn fx:id="at_dateCreated" prefWidth="120.0" text="Date Created" />
                        <TableColumn fx:id="at_createdBy" prefWidth="97.0" text="Created By" />
                    </columns>
                  </TableView>
               </content>
            </Tab>
          <Tab fx:id="closedtickets" text="Closed Tickets">
               <content>
                  <TableView prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="ct_id" prefWidth="75.0" text="ID" />
                      <TableColumn fx:id="ct_name" prefWidth="98.0" text="Name" />
                        <TableColumn fx:id="ct_priority" prefWidth="81.0" text="Priority" />
                        <TableColumn fx:id="ct_dateCreated" prefWidth="113.0" text="Date Created" />
                        <TableColumn fx:id="ct_dateClosed" prefWidth="115.0" text="Date Closed" />
                        <TableColumn fx:id="ct_createdBy" prefWidth="106.0" text="Created By" />
                    </columns>
                  </TableView>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
